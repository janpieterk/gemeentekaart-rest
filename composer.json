{
    "name": "janpieterk/gemeentekaart-rest",
    "type": "project",
    "description": "REST interface to janpieterk/gemeentekaart-core",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Jan Pieter Kunst",
            "email": "jpk@janpieterkunst.nl"
        }
    ],
    "keywords": [
        "gis",
        "geo"
    ],
    "homepage": "https://github.com/janpieterk/gemeentekaart-rest",
    "repositories": [
        {
            "type": "path",
            "url": "../gemeentekaart-core"
        }
    ],
    "require": {
        "janpieterk/gemeentekaart-core": "dev-develop",
        "php": ">=5.3.10",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-imagick": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4 <8",
        "squizlabs/php_codesniffer": ">=2.9"
    },
    "config": {
        "optimize-autoloader": true
    }
}
